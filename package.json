{
    "name": "vscode-inline-svg",
    "displayName": "Inline SVG",
    "description": "Inline SVG viewer",
    "version": "0.9.0",
    "publisher": "kontrail",
    "repository": {
        "type": "git",
        "url": "https://github.com/alexklyuev/vscode-inline-svg"
    },
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:inlineSvg.preview"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "inlineSvg.preview",
                "title": "Inline SVG: Preview inline svg"
            },
            {
                "command": "inlineSvg.zoomIn",
                "title": "Zoom In"
            },
            {
                "command": "inlineSvg.zoomOut",
                "title": "Zoom Out"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "inlineSvg.zoomIn",
                    "when": "inlineSvgPanelActive",
                    "group": "navigation"
                },
                {
                    "command": "inlineSvg.zoomOut",
                    "when": "inlineSvgPanelActive",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
