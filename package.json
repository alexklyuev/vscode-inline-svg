{
    "name": "vscode-inline-svg",
    "displayName": "vscode-inline-svg",
    "description": "",
    "version": "0.0.1",
    "publisher": "kontrail",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:inlineSvg.preview",
        "onCommand:inlineSvg.scan"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "inlineSvg.preview",
                "title": "Inline SVG: Preview inline svg"
            },
            {
                "command": "inlineSvg.scan",
                "title": "Inline SVG: Scan project for inline svg"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
